local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
-------------------------------------------------   
local window = lib.CreateLib("Lonely nuker V2 by lonelywhale&mob_zer", "DarkTheme")
-------------------------------------------------  
local player = game.Players.LocalPlayer
local backpack = player.Backpack
-------------------------------------------------  
local dupingtab = window:NewTab("Duping")
local nukingtab = window:NewTab("Nuking")
local freezingtab = window:NewTab("Projectile freezer")
local funtab = window:NewTab("Fun")
local section1 = dupingtab:NewSection("List of tools")
local section2 = nukingtab:NewSection("List of nuke tools")
local section3 = freezingtab:NewSection("List of tools")
local section4 = funtab:NewSection("Funny list")
-------------------------------------------------  
local c4amount
local grenadeamount
local firebombamount
local waterflaskamount
local poisonflaskamount


section1:NewLabel("More features will be coming soon.")
-------------------------------------------------  
section1:NewSlider("Amount of C4", "Choose how much", 300, 0, function(amount)
    c4amount = amount
end)

section1:NewSlider("Amount of Grenades", "Choose how much", 300, 0, function(amount)
    grenadeamount = amount
end)

section1:NewSlider("Amount of Fire bombs", "Choose how much", 300, 0, function(amount)
    firebombamount = amount
end)

section1:NewSlider("Amount of Water flasks", "Choose how much", 300, 0, function(amount)
    waterflaskamount = amount
end)

section1:NewSlider("Amount of Poison flasks", "Choose how much", 300, 0, function(amount)
    poisonflaskamount = amount
end)
-------------------------------------------------  
section1:NewButton("Dupe C4", "Click me to dupe", function()
    for i = 1, c4amount do
            local args = {
            [1] = 0,
            [2] = "0:0:0:0" -- DO NOT CHANGE ME OR IT WILL CHANGE THE TOOLS
        }

        game:GetService("ReplicatedStorage"):WaitForChild("Item"):FireServer(unpack(args))
    end
    task.wait(6)
    for _, v in pairs(backpack:GetChildren()) do
        if v:IsA("Tool") and v.Name ~= "C4" then
            v:Destroy()
        end
    end
end)

section1:NewButton("Dupe Grenades", "Click me to dupe", function()
    for i = 1, grenadeamount do
            local args = {
            [1] = 0,
            [2] = "0:0:0:0" -- DO NOT CHANGE ME OR IT WILL CHANGE THE TOOLS
        }

        game:GetService("ReplicatedStorage"):WaitForChild("Item"):FireServer(unpack(args))
    end
    task.wait(6)
    for _, v in pairs(backpack:GetChildren()) do
        if v:IsA("Tool") and v.Name ~= "Grenade" then
            v:Destroy()
        end
    end
end)

