section4:NewButton("Kamikaze (Requires grenade)", "Literally fucking explode", function()
    local grenade = backpack:FindFirstChild("Grenade")
    grenade.Parent = player.Character
    grenade.start:FireServer()
    wait(3)
    local args = {
        [1] = game.Players.LocalPlayer.Character["Left Arm"].Position + game.Players.LocalPlayer.Character.Torso.Position,
        [2] = 1686851243.277296,
        [3] = game.Players.LocalPlayer.Character["Right Arm"].Position + game.Players.LocalPlayer.Character.Head.Position
        }
    
    grenade.throw:FireServer(unpack(args))
end)

section4:NewButton("Tsar bomba (Requires C4)", "Destruction", function()
    for _, v in pairs(backpack:GetChildren()) do
        if v:IsA("Tool") and v.Name == "C4" then
           v:Activate()
        end
     end
end)

section4:NewButton("Flame thrower (Requires Fire bomb)", "OH GOD", function()
    for i = 1, 40 do
        local firebomb = backpack:FindFirstChild("Fire bomb")
        firebomb.Parent = player.Character
        firebomb.start:FireServer()
        wait(3)
        local args = {
        [1] = game.Players.LocalPlayer.Character.Torso.Position + Vector3.new(math.random(1,20), math.random(1,15), math.random(1,20)),
        [2] = 1686851243.277296,
        [3] = game.Players.LocalPlayer.Character.Head.Position + Vector3.new(math.random(1,20), math.random(1,17), math.random(1,20))
        }
    
        firebomb.throw:FireServer(unpack(args))
        wait(1)
    end
end)

