section3:NewButton("Grenade freeze", "Freeze grenades", function()
    local grenade = backpack:FindFirstChild("Grenade")
    grenade.Parent = player.Character
    for method1 = 1, 50 do
        grenade.start:FireServer()
    end
    wait(3)
    for method2 = 1, 50 do
    local args = {
        [1] = game.Players.LocalPlayer.Character.Torso.Position - Vector3.new(math.random(1,50), math.random(1,50), math.random(1,50)),
        [2] = 1686851243.277296,
        [3] = game.Players.LocalPlayer.Character.Head.Position - Vector3.new(math.random(1,50), math.random(1,50), math.random(1,50))
        }
    
    grenade.throw:FireServer(unpack(args))
    end
end)

section3:NewButton("Fire bomb freeze", "Freeze fire bombs", function()
    local firebomb = backpack:FindFirstChild("Fire bomb")
    firebomb.Parent = player.Character
    for method1 = 1, 50 do
        firebomb.start:FireServer()
    end
    wait(3)
    for method2 = 1, 50 do
    local args = {
        [1] = game.Players.LocalPlayer.Character.Torso.Position - Vector3.new(math.random(1,50), math.random(1,50), math.random(1,50)),
        [2] = 1686851243.277296,
        [3] = game.Players.LocalPlayer.Character.Head.Position - Vector3.new(math.random(1,50), math.random(1,50), math.random(1,50))
        }
    
        firebomb.throw:FireServer(unpack(args))
    end
end)

section3:NewButton("Water flask freeze", "Freeze water flasks", function()
    local water = backpack:FindFirstChild("Water flask")
    water.Parent = player.Character
    for method1 = 1, 50 do
        water.start:FireServer()
    end
    wait(3)
    for method2 = 1, 50 do
    local args = {
