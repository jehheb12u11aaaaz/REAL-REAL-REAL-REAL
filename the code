section1:NewButton("Dupe Water flasks", "Click me to dupe", function()
    for i = 1, waterflaskamount do
            local args = {
            [1] = 0,
            [2] = "0:0:2:0" -- DO NOT CHANGE ME OR IT WILL CHANGE THE TOOLS
