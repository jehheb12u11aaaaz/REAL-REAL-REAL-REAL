            v:Destroy()
        end
    end
end)

section1:NewButton("Dupe Poison flasks", "Click me to dupe", function()
    for i = 1, poisonflaskamount do
            local args = {
            [1] = 0,
            [2] = "0:0:1:0" -- DO NOT CHANGE ME OR IT WILL CHANGE THE TOOLS
        }

        game:GetService("ReplicatedStorage"):WaitForChild("Item"):FireServer(unpack(args))
    end
    task.wait(6)
    for _, v in pairs(backpack:GetChildren()) do
        if v:IsA("Tool") and v.Name ~= "Poison flask" then
            v:Destroy()
        end
    end
end)
-------------------------------------------------  

section2:NewButton("C4 Nuke", "Spams C4's", function()
    for _, v in pairs(backpack:GetChildren()) do
       if v:IsA("Tool") and v.Name == "C4" then
          v:Activate()
          task.wait(0.1)
       end
    end
end)


section2:NewButton("Grenade nuke", "Spam grenades", function()
    for _, v in pairs(backpack:GetChildren()) do
        if v:IsA("Tool") and v.Name == "Grenade" then
           v.start:FireServer()
        end
        wait(3)
        local args = {
            [1] = game.Players.LocalPlayer.Character.Torso.Position + Vector3.new(14,3,13)
            [2] = 1686851243.277296,
            [3] = game.Players.LocalPlayer.Character.Head.Position + Vector3.new(15,4,12)
        }
        if v:IsA("Tool") and v.Name == "Grenade" then
            v.throw:FireServer(unpack(args))
         end
    end
end)


section2:NewButton("Fire bomb nuke", "Spam fire bombs", function()
    for _, v in pairs(backpack:GetChildren()) do
        if v:IsA("Tool") and v.Name == "Fire bomb" then
           v.start:FireServer()
        end
        wait(3)
        local args = {
